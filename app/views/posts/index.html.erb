<h1>Posts#index</h1>
<p>Find me in app/views/posts/index.html.erb</p>
<h1> All Those Posts</h1>

<% @posts.each do |post| %>
<div class="media">
    <div class="media-body">
      <h4 class="media-heading">
        <p><%= link_to post.title, post_path(post.id) %></p>
      </h4>
    </div>
  </div>
<% end %>
